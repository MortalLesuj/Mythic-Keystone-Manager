					<TextHeight val="5"/>
					<Anchors>
						<Anchor point="LEFT" relativePoint="LEFT"> 
							<Offset x="5" y="0" />
						</Anchor>
					</Anchors>
				</FontString>
			</Layer>
		</Layers>
		<Frames>
			<Button name="$parrentInviteButton" text="Invite" parentKey="InviteButton" inherits="UIPanelButtonTemplate">
				<Size x="60" y="20"/>
				<Anchors>
					<Anchor point="RIGHT" relativePoint="RIGHT">
						<Offset x="-5" y="0"/>
					</Anchor>
				</Anchors>
			</Button>
		</Frames>
	</Frame>
